{
  "name": "image-url-renderer",
  "private": true,
  "version": "0.0.0",
  "homepage": "https://veejaliu.github.io/ImageUrlRenderer",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev-expose": "vite --host --port 10101 --mode development",
    "prod-expose": "vite --host --port 10101 --mode production",
    "build": "tsc && vite build",
    "lint": "eslint antd-demo --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier-format": "prettier --write .",
    "predeploy" : "npm run build",
    "deploy" : "gh-pages -d dist"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "gh-pages": "^6.2.0",
    "prettier": "^3.2.2",
    "typescript": "^5.0.2",
    "vite": "5.2.6"
  }
}
